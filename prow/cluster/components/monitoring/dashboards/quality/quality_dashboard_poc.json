          "rawQuery": true,
          "rawSql": "WITH issues_filtered AS\n(\n    (\n        SELECT DISTINCT \n            repository.name as repository,\n            number,\n            labels_unpacked.name as label,\n            created_at,\n            closed_at\n        FROM `sap-kyma-prow.github_issues.github_com_kyma_project`, UNNEST(labels) as labels_unpacked\n        WHERE repository.name IN (\"kyma\", \"control-plane\")\n    )\n    UNION ALL\n    (\n        SELECT DISTINCT\n            repository.name as repository,\n            number,\n            labels_unpacked.name as label,\n            created_at,\n            closed_at\n        FROM `sap-kyma-prow.github_issues.github_tools_sap_kyma`, UNNEST(labels) as labels_unpacked\n        WHERE repository.name = \"backlog\"\n    )\n)\nSELECT\n    IFNULL(issues_open.area, issues_prev.area) as area,\n    IFNULL(issues_prev.cnt, 0) as previous,\n    IFNULL(issues_open.cnt, 0) as open\nFROM\n(\n    SELECT \n        SUBSTR(label,6) as area,\n        COUNT(*) as `cnt`\n    FROM issues_filtered\n    WHERE label LIKE \"area/%\"\n    AND (number, repository) IN (SELECT (number, repository) FROM issues_filtered WHERE label = \"bug\")\n    AND (closed_at IS NULL OR closed_at > $__millisTimeTo(closed_at))\n    AND created_at < $__millisTimeTo(created_at)\n    GROUP BY label\n    ORDER BY area\n) as issues_open\nFULL JOIN\n(\n    SELECT \n      SUBSTR(label,6) as area,\n      COUNT(*) as `cnt`\n    FROM issues_filtered\n    WHERE label LIKE \"area/%\"\n    AND (number, repository) IN (SELECT (number, repository) FROM issues_filtered WHERE label = \"bug\")\n    -- get all issues that were created BEFORE the selected period, and still open or closed AFTER/DURING selected period\n    AND (closed_at IS NULL OR closed_at > $__millisTimeFrom(closed_at))\n    AND created_at < $__millisTimeFrom(closed_at)\n    GROUP BY label\n    ORDER BY area\n) as issues_prev\nON issues_open.area = issues_prev.area\n",
