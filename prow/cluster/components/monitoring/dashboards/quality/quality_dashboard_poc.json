          "rawSql": "WITH all_jobs AS\n(\n    SELECT\n        jsonPayload.job as job,\n        jsonPayload.to as state\n    FROM `sap-kyma-prow.prow_jobs.stderr`\n    WHERE\n        --TIMESTAMP BETWEEN \"2021-05-13\" AND \"2021-05-21\"\n        timestamp >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY)\n        AND jsonPayload.type != \"presubmit\"\n        AND jsonPayload.to IN ( \"success\", \"error\", \"failure\")\n)\nSELECT \n    job,\n    COUNTIF(state = \"success\") as `cnt_success`,\n    COUNT(*) as `cnt`,\n    COUNTIF(state = \"success\") / COUNT(*) * 100 as `success`\nFROM all_jobs \nGROUP BY job\nORDER BY job",
