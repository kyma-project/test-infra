{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/object1688725677.json",
  "title": "Root",
  "type": "object",
  "required": [
    "images"
  ],
  "properties": {
    "images": {
      "$id": "#root/images",
      "title": "Images",
      "type": "array",
      "default": [],
      "items":{
        "$id": "#root/images/items",
        "title": "Items",
        "type": "object",
        "required": [
          "registry",
          "purpose",
          "publisher",
          "sourceCode",
          "contributors",
          "activity",
          "comment",
          "openSSF"
        ],
        "properties": {
          "registry": {
            "$id": "#root/images/items/registry",
            "title": "Registry",
            "type": "string",
            "examples": [
              "gcr.io/k8s-prow/*"
            ]
          },
          "purpose": {
            "$id": "#root/images/items/purpose",
            "title": "Purpose",
            "type": "string",
            "examples": [
              "Prow CI"
            ]
          },
          "publisher": {
            "$id": "#root/images/items/publisher",
            "title": "Publisher",
            "type": "string",
            "examples": [
              "Prow community"
            ]
          },
          "sourceCode": {
            "$id": "#root/images/items/sourceCode",
            "title": "Sourcecode",
            "type": "string",
            "examples": [
              "https://github.com/kubernetes/test-infra/tree/master/prow"
            ]
          },
          "contributors": {
            "$id": "#root/images/items/contributors",
            "title": "Contributors",
            "type": "integer"
          },
          "activity": {
            "$id": "#root/images/items/activity",
            "title": "Activity",
            "type": "object",
            "required": [
              "frequency",
              "evidence"
            ],
            "properties": {
              "frequency": {
                "$id": "#root/images/items/activity/frequency",
                "title": "Frequency",
                "type": "string",
                "enum": ["daily", "weekly", "monthly"]
              },
              "evidence": {
                "$id": "#root/images/items/activity/evidence",
                "title": "Evidence",
                "type": "string",
                "default": "",
                "examples": [
                  "https://github.com/kubernetes/test-infra/commits/master/prow"
                ]
              }
            }
          }
        ,
          "comment": {
            "$id": "#root/images/items/comment",
            "title": "Comment",
            "type": "string",
            "default": ""
          },
          "openSSF": {
            "$id": "#root/images/items/openSSF",
            "title": "Openssf",
            "type": "object",
            "required": [
              "score",
              "review"
            ],
            "properties": {
              "score": {
                "$id": "#root/images/items/openSSF/score",
                "title": "Score",
                "type": "number",
                "examples": [
                  5.6
                ],
                "default": 0.0
              },
              "review": {
                "$id": "#root/images/items/openSSF/review",
                "title": "Review",
                "type": "object",
                "required": [
                  "approvedImage",
                  "reason"
                ],
                "properties": {
                  "approvedImage": {
                    "$id": "#root/images/items/openSSF/review/approvedImage",
                    "title": "Approvedimage",
                    "type": "boolean",
                    "default": false
                  },
                  "reason": {
                    "$id": "#root/images/items/openSSF/review/reason",
                    "title": "Reason",
                    "type": "string",
                    "default": ""
                  }
                }
              }

            }
          }

        }
      }

    }
  }
}
